{
    "name": "cortex/auth",
    "description": "Cortex Auth is a frontend layer for the powerful authentication, authorization and verification package rinvex/laravel-auth on top of Laravel. It has all required controllers, views, routes, and other required assets to run a fully functional user management system with complete dashboard out of the box.",
    "type": "cortex-module",
    "keywords": [
        "authentication",
        "authorization",
        "verification",
        "activation",
        "abilities",
        "cortex",
        "phone",
        "sms",
        "call",
        "users",
        "module",
        "security",
        "twofactor",
        "laravel",
        "rinvex",
        "rbac",
        "totp",
        "acl",
        "roles"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-auth/issues",
        "source": "https://github.com/rinvex/cortex-auth",
        "docs": "https://github.com/rinvex/cortex-auth/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-auth/contributors"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "illuminate/auth": "^10.0.0 || ^11.0.0",
        "illuminate/broadcasting": "^10.0.0 || ^11.0.0",
        "illuminate/bus": "^10.0.0 || ^11.0.0",
        "illuminate/console": "^10.0.0 || ^11.0.0",
        "illuminate/contracts": "^10.0.0 || ^11.0.0",
        "illuminate/database": "^10.0.0 || ^11.0.0",
        "illuminate/http": "^10.0.0 || ^11.0.0",
        "illuminate/notifications": "^10.0.0 || ^11.0.0",
        "illuminate/queue": "^10.0.0 || ^11.0.0",
        "illuminate/routing": "^10.0.0 || ^11.0.0",
        "illuminate/session": "^10.0.0 || ^11.0.0",
        "illuminate/support": "^10.0.0 || ^11.0.0",
        "illuminate/validation": "^10.0.0 || ^11.0.0",
        "cortex/foundation": "^8.0.0",
        "diglactic/laravel-breadcrumbs": "^8.0.0",
        "laravel/socialite": "^5.6.0",
        "laravelcollective/html": "^6.3.0",
        "laravel-notification-channels/authy": "^10.0.0",
        "league/fractal": "^0.20.0",
        "pragmarx/google2fa": "^8.0.0",
        "proengsoft/laravel-jsvalidation": "^4.8.0",
        "propaganistas/laravel-phone": "^5.0.0",
        "rinvex/laravel-auth": "^7.0.0",
        "rinvex/laravel-composer": "^7.0.0",
        "rinvex/countries": "^9.0.0",
        "rinvex/languages": "^7.0.0",
        "rinvex/laravel-menus": "^7.0.0",
        "rinvex/laravel-support": "^7.0.0",
        "rinvex/laravel-tags": "^7.0.0",
        "rinvex/tmp-josephsilber-bouncer": "^1.0.0",
        "spatie/laravel-activitylog": "^4.7.0",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-schemaless-attributes": "^2.4.0",
        "yajra/laravel-datatables-buttons": "^10.0.0",
        "yajra/laravel-datatables-fractal": "^10.0.0",
        "yajra/laravel-datatables-html": "^11.4.0",
        "yajra/laravel-datatables-oracle": "^10.4.0",
        "symfony/console": "^6.2.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.32.0",
        "phpunit/phpunit": "^10.1.0"
    },
    "autoload": {
        "psr-4": {
            "Cortex\\Auth\\": "src/",
            "Cortex\\Auth\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Auth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cortex\\Auth\\Providers\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
