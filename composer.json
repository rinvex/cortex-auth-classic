{
    "name": "cortex/auth",
    "description": "Cortex Auth is a frontend layer for the powerful authentication, authorization and verification package rinvex/laravel-auth on top of Laravel. It has all required controllers, views, routes, and other required assets to run a fully functional user management system with complete dashboard out of the box.",
    "type": "cortex-module",
    "keywords": [
        "authentication",
        "authorization",
        "verification",
        "activation",
        "abilities",
        "cortex",
        "phone",
        "sms",
        "call",
        "users",
        "module",
        "security",
        "twofactor",
        "laravel",
        "rinvex",
        "rbac",
        "totp",
        "acl",
        "roles"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-auth/issues",
        "source": "https://github.com/rinvex/cortex-auth",
        "docs": "https://github.com/rinvex/cortex-auth/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-auth/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "Illuminate/auth": "~5.7.0",
        "Illuminate/console": "~5.7.0",
        "Illuminate/database": "~5.7.0",
        "Illuminate/http": "~5.7.0",
        "Illuminate/routing": "~5.7.0",
        "Illuminate/support": "~5.7.0",
        "cortex/foundation": "^1.0.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.2.0",
        "laravel/socialite": "^3.0.0",
        "laravelcollective/html": "~5.7.0",
        "league/fractal": "^0.17.0",
        "pragmarx/google2fa": "^4.0.0",
        "proengsoft/laravel-jsvalidation": "^2.3.0",
        "propaganistas/laravel-phone": "^4.1.0",
        "rinvex/laravel-auth": "^1.0.0",
        "rinvex/laravel-composer": "^1.0.0",
        "rinvex/countries": "^5.0.0",
        "rinvex/laravel-forms": "^1.0.0",
        "rinvex/languages": "^3.0.0",
        "rinvex/laravel-menus": "^1.0.0",
        "rinvex/laravel-support": "^1.0.0",
        "rinvex/laravel-tenants": "^1.0.0",
        "silber/bouncer": "v1.0.0-rc.4",
        "spatie/laravel-activitylog": "^3.1.0",
        "spatie/laravel-schemaless-attributes": "~1.3.0",
        "yajra/laravel-datatables-buttons": "^4.5.0",
        "yajra/laravel-datatables-fractal": "^1.3.0",
        "yajra/laravel-datatables-html": "^4.3.0",
        "yajra/laravel-datatables-oracle": "^8.13.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.23.0",
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Cortex\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Auth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cortex\\Auth\\Providers\\AuthServiceProvider",
                "Cortex\\Auth\\Overrides\\Silber\\Bouncer\\BouncerServiceProvider"
            ],
            "aliases": {
                "Bouncer": "Silber\\Bouncer\\BouncerFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
